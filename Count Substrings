class Solution
{
    int countSubstr (String S)
    {
        // your code here    
        
        // your code here   
        //char[]ch = new int[S.length];
        int count=0;
        for(int i=0;i<S.length();i++){
            //char chh = S.charAt(i);
            if(S.charAt(i) == '1'){
                count++;
            }
        }
        int res = (count*(count-1)/2);
        return res;
    }
}
